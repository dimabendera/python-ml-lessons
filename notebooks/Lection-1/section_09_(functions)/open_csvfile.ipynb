    "def open_csvfile(filename, delimiter=','):\n",
    "\n",
    "    with open(filename, \"r\") as csv_file:        \n",
    "        rows = csv_file.read().split(\"\\n\")\n",
    "\n",
    "    for index, row in enumerate(rows):\n",
    "        rows[index] = row.split(delimiter)\n",
    "\n",
    "    return rows"
   ]
  }
